{
  "name": "@hapify/ejs-vm",
  "version": "0.3.4",
  "description": "Sandbox for EJS templates",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "update": "npm-check -u",
    "test": "nyc mocha",
    "lint": "prettier --write '**/*.ts'",
    "build": "rimraf dist && tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:hapify/ejs-vm.git"
  },
  "keywords": [
    "hapify",
    "ejs",
    "vm"
  ],
  "author": "Edouard Demotes-Mainard <edouard@tractr.net>",
  "license": "MIT",
  "homepage": "https://github.com/hapify/ejs-vm#readme",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@hapi/code": "^8.0.2",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "husky": "^4.3.0",
    "mocha": "^8.2.1",
    "npm-check": "^5.9.2",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@hapify/vm": "^0.3.4"
  },
  "engines": {
    "node": ">=9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --pattern \"**/*.ts\""
    }
  }
}
